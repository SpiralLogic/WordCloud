<Window x:Class="WordCloudTest.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WordCloudTest.Views"
        mc:Ignorable="d"
        Title="MainWindow" Height="600" Width="600">
    <StackPanel>
        <local:WordCloud2 DataContext="{Binding WordCloudData}" x:Name="WordcloudControl" Height="500" Width="500"></local:WordCloud2>
        <Button Click="DoWordCloud" Content="do"></Button>
    </StackPanel>
</Window>
