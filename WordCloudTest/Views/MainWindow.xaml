<Window x:Class="WordCloudTest.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WordCloudTest.Views"
        mc:Ignorable="d"
        Title="MainWindow" Height="900" Width="900" BorderThickness="0">
    <StackPanel>
        <local:WordCloud2 DataContext="{Binding WordCloudData}" x:Name="WordcloudControl" Height="800" Width="800"></local:WordCloud2>
        <Button Click="AddWord" Content="add"></Button>
        <Button Click="DoWordCloud" Content="do"></Button>
    </StackPanel>
</Window>
